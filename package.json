{
  "name": "redim",
  "displayName": "Redim",
  "description": "High performance image processing easily by using the command line.",
  "version": "2.0.1",
  "publisher": "Redimvs",
  "icon": "Redim_logo.png",
  "pricing": "Free",
  "license":"Apache-2.0",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "redim.redim",
        "title": "Redim Optimizer"
      },
      {
        "command": "redim.optimizeImageWithOptions",
        "title": "Optimize Image (With Options)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "redim.optimizeImageWithOptions",
          "when": "resourceLangId == image || resourceExt == .png || resourceExt == .jpg || resourceExt == .jpeg || resourceExt == .webp",
          "group": "image-optimization"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "axios": "^1.7.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "sharp": "^0.33.5"
  }
}
